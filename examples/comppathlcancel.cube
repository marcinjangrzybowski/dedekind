p : (i)[]
q : (i)[i = 0 -> p(0)]
---
? : (i j)[i = 0 -> q(j) |
          i = 1 -> hcomp (k)[j = 0 -> p(1) |
                             j = 1 -> q(k) ]
                                hcomp (k)[j = 0 -> p(k) | j = 1 -> p(0)] p(0) |
          j = 0 -> p(i) |
          j = 1 -> q(1) ]


? : (i j)[i = 0 -> q(j) |
          i = 1 -> hcomp (k)[j = 0 -> p(0) |
                             j = 1 -> hcomp (l)[k = 0 -> p(1) |
                                                k = 1 -> q(l) ] hcomp (m)[k = 0 -> p(m) | k = 1 -> p(0)] p(0) ]
                                p(j) |
          j = 0 -> q(0) |
